{
"manifest_version": 3,
"name": "Social Links Quickshare",
"description": "Quickly view & copy your profiles, portfolio and social links.",
"version": "1.0",
"action": {
  "default_popup": "popup.html",
  "default_title": "Social Links Quickshare",
  "default_icon": {
    "16": "icons/link-icon-16.png",
    "48": "icons/link-icon-48.png",
    "128": "icons/link-icon-128.png"
  }
},
"icons": {
  "16": "icons/link-icon-16.png",
  "48": "icons/link-icon-48.png",
  "128": "icons/link-icon-128.png"
},
"permissions": [
"storage"
]
}